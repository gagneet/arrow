{
    "autos.yahoo.com" : "localhost",

    "localhost": {
        "newHost": "localhost",
        "headers":
                [
                    { "param": "User-Agent",
                        "value": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10.7; rv:16.0) Gecko/20100101 Firefox/16.0"
                    },

                    { "param": "Accept-Language",
                        "value": "en-US,en;q=0.5"
                    },

                    { "param": "Accept-Encoding",
                        "value": "gzip, deflate"
                    },
                    { "param": "Accept",
                        "value": "text/html,application/xhtml+xml,application/xml;q=0.9,*/*;q=0.8"
                    },

                    { "param": "Proxy-Connection",
                        "value": "keep-alive"
                    }
                ],
        "record": true
    },
    "coverage":{
        "clientSideCoverage": true,
        "coverageExclude": ["^http://yui.yahooapis.com.*\\.js$",".*login.yahoo.com"]
    }

}
